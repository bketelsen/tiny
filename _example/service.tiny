service="users"
description="User Management Service"

type GetRequest {
  uid int
}

type GetResponse {
  first_name string
  last_name string
}

type UnlockRequest {
  uid int
}

type UnlockResponse {
}

endpoint User {
  rpc Get(GetRequest) returns (GetResponse)
  rpc Unlock(UnlockRequest) returns (UnlockResponse)
}
